{
  "name": "ai-cm",
  "version": "2.0.3",
  "description": "CryptoMiner Pro - AI-Powered Cryptocurrency Mining Dashboard",
  "main": "backend-nodejs/server.js",
  "scripts": {
    "install:backend": "cd backend-nodejs && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "start:backend": "cd backend-nodejs && npm start",
    "start:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "test": "./scripts/test-installation.sh",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "setup": "./scripts/install-modern.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nmdata575/ai-cm.git",
    "branch": "release-2.03"
  },
  "keywords": [
    "cryptocurrency",
    "mining",
    "bitcoin",
    "litecoin",
    "dogecoin",
    "scrypt",
    "ai",
    "machine-learning",
    "nodejs",
    "react",
    "mongodb",
    "dashboard",
    "real-time",
    "mining-pool",
    "blockchain"
  ],
  "author": {
    "name": "nmdata575",
    "url": "https://github.com/nmdata575"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nmdata575/ai-cm/issues"
  },
  "homepage": "https://github.com/nmdata575/ai-cm/tree/release-2.03#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "workspaces": [
    "backend-nodejs",
    "frontend"
  ]
}